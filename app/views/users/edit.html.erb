<h1>Hello world</h1>
<%= form_with url:  user_path(@user.id), method: :patch,local: true do |form| %>
    <table class="table">
        <tbody>
            <tr>
                <td> <h3>Role: </h3> <%= @user.role %> </td>
                <td> <%= form.select :role, options_for_select([["Select Role", ""],["manager", "manager"], ["qa", "qa"], ["developer", "developer"]]) %> </td> 
                <td><%= form.submit  %></td>
            </tr>
        </tbody>
    </table>
<% end %>
